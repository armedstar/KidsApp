
function CESUser(id, username, displayName)
{
    this.id = id;
    this.username = username;
    this.displayName = displayName;
}

CESUser.prototype.avatarLoginImageSrc = null;
CESUser.prototype.avatarImageSrc = null;
CESUser.prototype.avatarFrameImageSrc = null;
CESUser.prototype.avatarTextColor = null;

CESUser.prototype.toString = function()
{
    return JSON.stringify(this);
}
