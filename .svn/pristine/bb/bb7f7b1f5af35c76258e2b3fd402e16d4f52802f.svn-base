jingo.declare(
{
    require: [],
    name: 'shared.model.Theme',
    as: function() 
    {

        shared.model.Theme = function(
            id, 
            displayName, 
            selectionIconImageSrc, 
            iconImageSrc,
            backgroundImageSrc,  
            nameplateIconImageSrc,
            nameplateFontColor,  
            nameplateCloseButtonImageSrc,
            programTileFontColor,  
            videoPlayerFontColor, 
            videoPlayerScrubberButton,
            videoPlayerEpisodeCloseButton)
        {
            this.id = id;
            this.displayName = displayName;
            this.selectionIconImageSrc = selectionIconImageSrc;
            this.iconImageSrc = iconImageSrc;
            this.backgroundImageSrc = backgroundImageSrc;
            this.nameplateIconImageSrc = nameplateIconImageSrc;
            this.nameplateFontColor = nameplateFontColor;
            this.nameplateCloseButtonImageSrc = nameplateCloseButtonImageSrc;
            this.programTileFontColor = programTileFontColor;
            this.videoPlayerFontColor = videoPlayerFontColor;
            this.videoPlayerScrubberButton = videoPlayerScrubberButton;
            this.videoPlayerEpisodeCloseButton = videoPlayerEpisodeCloseButton;
        }
        
        shared.model.Theme.prototype = {};
        shared.model.Theme.prototype.parent = Object.prototype;
        shared.model.Theme.prototype.constructor = shared.model.Theme;

        shared.model.Theme.prototype.id = null;
        shared.model.Theme.prototype.displayName = null;
        shared.model.Theme.prototype.selectionIconImageSrc = null;
        shared.model.Theme.prototype.iconImageSrc = null;
        shared.model.Theme.prototype.backgroundTileImageSrc = null;
        shared.model.Theme.prototype.nameplateIconImageSrc = null;
        shared.model.Theme.prototype.nameplateFontColor = null;
        shared.model.Theme.prototype.nameplateCloseButtonImageSrc = null;
        shared.model.Theme.prototype.programTileFontColor = null;
        shared.model.Theme.prototype.videoPlayerFontColor = null;
        shared.model.Theme.prototype.videoPlayerScrubberButton = null;
        shared.model.Theme.prototype.videoPlayerEpisodeCloseButton = null;

        shared.model.Theme.prototype.toString = function()
        {
            return JSON.stringify(this);
        }
        
    }
});
